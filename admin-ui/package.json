{
  "name": "admin-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "lint": "next lint",
    "lint:css": "stylelint './src/**/*.tsx'",
    "format": "prettier --ignore-path \"../.prettierignore\" --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "dev": "node server.cjs",
    "test": "TZ=Europe/Helsinki react-app-rewired test",
    "test:e2e-local": "testcafe --skip-js-errors \"chrome --window-size='1920,1080'\" e2e/tests",
    "ts-check": "tsc --noEmit --project tsconfig.tscheck.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write",
    "*.{js,ts,tsx}": "eslint --cache",
    "*.{tsx,css,scss}": "stylelint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "^app(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
