    "@types/react-big-calendar": "^0.38.3",
    "apollo-link-token-refresh": "^0.4.0",
    "apollo-upload-client": "17.0.0",
    "axios": "^1.2.1",
    "axios-auth-refresh": "^3.3.4",
    "axios-case-converter": "^0.11.1",
