{
  "name": "admin-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "next dev -p 3001",
    "build": "SKIP_ENV_VALIDATION=true next build",
    "start": "next start",
    "lint": "SKIP_ENV_VALIDATION=true next lint",
    "lint:fix": "SKIP_ENV_VALIDATION=true next lint --fix",
    "lint:css": "SKIP_ENV_VALIDATION=true stylelint './src/**/*.tsx'",
    "format": "SKIP_ENV_VALIDATION=true prettier --ignore-path \"../.prettierignore\" --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "test": "SKIP_ENV_VALIDATION=true TZ=Europe/Helsinki jest --runInBand",
    "test:watch": "SKIP_ENV_VALIDATION=true jest --watch",
    "tsc:check": "tsc --noEmit --project tsconfig.tscheck.json",
    "tsc:clean": "tsc --build --clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write",
    "*.{js,ts,tsx}": "eslint --cache",
    "*.{tsx,css,scss}": "stylelint"
  },
