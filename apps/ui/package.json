{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "next dev -p 3000",
    "dev:test": "NEXT_PUBLIC_MOCK_REQUESTS=true DISABLE_AUTH=true NEXT_PUBLIC_BASE_URL=http://localhost:4000 NEXT_PUBLIC_TILAVARAUS_API_URL=http://localhost:4000 next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "test": "TZ=Europe/Helsinki jest --runInBand",
    "test:watch": "jest --watch",
    "test:browser:ci": "CYPRESS_BASE_URL=http://localhost:4000 TZ=Europe/Helsinki cypress run --config-file='cypress.config.ts' --browser chrome",
    "test:browser": "CYPRESS_BASE_URL=http://localhost:4000 TZ=Europe/Helsinki cypress run --config-file='cypress.config.ts' --browser chrome",
    "test:browser:open": "CYPRESS_BASE_URL=http://localhost:4000 TZ=Europe/Helsinki cypress open --config-file='cypress.config.ts' --browser chrome",
    "test:browser:headed": "CYPRESS_BASE_URL=http://localhost:4000 TZ=Europe/Helsinki cypress run --config-file='cypress.config.ts' --browser chrome --headed",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:css": "stylelint '**/*.tsx'",
    "tsc:check": "tsc --noEmit --project tsconfig.json",
    "tsc:clean": "tsc --build --clean",
    "format": "prettier --ignore-path \"../.prettierignore\" --write \"**/*.{js,jsx,ts,tsx,json}\""
  },
