{
  "name": "common",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./index.ts",
  "types": "./dist/index.d.ts",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint './src/**/*.tsx'",
    "test": "echo 'FIXME common tests are broken'",
    "format": "prettier --ignore-path \"../.prettierignore\" --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "generate-email-templates": "node ./email/convertToHTML.mjs",
    "tsc:check": "tsc --noEmit --project tsconfig.tscheck.json",
    "tsc:clean": "tsc --build --clean",
    "codegen": "pnpm codegen:schema && pnpm codegen:types && pnpm generate-email-templates",
    "codegen:schema": "npx --yes get-graphql-schema http://localhost:8000/graphql/ > ../../tilavaraus.graphql",
    "codegen:types": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "eslint-config-custom": "workspace:*",
    "hds-react": "^2.17.0",
    "i18next": "^23.6.0",
    "lodash": "^4.17.21",
    "mjml": "^4.14.1",
    "next": "13.5.6",
    "next-i18next": "^14.0.3",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^13.3.1",
    "react-use": "^17.4.2",
    "styled-components": "5.3.11",
    "tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.0.27",
    "@types/react-big-calendar": "^1.8.8",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "^5.1.29",
    "@vercel/style-guide": "^5.1.0",
    "chalk": "^5.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mkdirp": "^3.0.1",
    "prettier": "^3.1.1",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
